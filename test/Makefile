OBJS = tokenize.cmo

tokenize: $(OBJS)
	ocamlc -o tokenize $(OBJS)

%.cmo: %.ml
	ocamlc -c $<

%.cmi: %.mli
	ocamlc -c $<

.PHONY: clean
clean:
	rm -f tokenize scanner.ml *.cmo *.cmi

# Generated by ocamldep *.ml *.mli
tokenize.cmo: ../compiler/scanner.cmo ../compiler/tokens.cmi 
tokenize.cmx: ../compiler/scanner.cmx