# Odds: test/semantic_analysis Makefile
#  - builds tand executes the semantic_analysis printer

OCAMLC = ocamlc
OBJS = ../../compiler/parser.cmo ../../compiler/scanner.cmo ../../compiler/analyzer.cmo
INCLUDES = -I ../../compiler

default: sast_print

all:
	cd ..; make all

sast_print: $(OBJS)
	$(OCAMLC) $(INCLUDES) -o sast_printer $(OBJS)

%.cmo: %.ml
	$(OCAMLC) $(INCLUDES) -c $<

%.cmi: %.mli
	$(OCAMLC) $(INCLUDES) -c $<

.PHONY: clean
clean:
	rm -f sast_printer *.cmo *.cmi

# Generated by ocamldep *.ml
sast_printer.cmo:
sast_printer.cmx:
